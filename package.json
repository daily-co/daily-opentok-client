{
  "name": "daily-tokbox",
  "version": "0.0.0",
  "description": "Daily Tokbox shim",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/daily-tokbox.umd.js",
  "module": "./dist/daily-tokbox.es.js",
  "exports": {
    ".": {
      "import": "./dist/daily-tokbox.es.js",
      "require": "./dist/daily-tokbox.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "vite",
