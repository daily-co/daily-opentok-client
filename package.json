    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "2.7.1",
    "typescript": "^4.7.4",
    "vite": "^2.4.4",
    "vite-plugin-mkcert": "^1.9.0"
